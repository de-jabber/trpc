{
  "name": "@trpc/upgrade",
  "version": "0.0.0-alpha.4",
  "description": "Upgrade scripts for tRPC",
  "author": "juliusmarminge",
  "license": "MIT",
  "bin": "./dist/cli.cjs",
  "homepage": "https://trpc.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trpc/trpc.git",
    "directory": "packages/upgrade"
  },
  "exports": {
    "./transforms/hooksToOptions": {
      "require": "./dist/transforms/hooksToOptions.cjs"
    },
    "./transforms/provider": {
      "require": "./dist/transforms/provider.cjs"
    }
  },
  "scripts": {
    "build": "bunchee",
    "run": "tsx src/cli.ts"
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "package.json",
    "!**/*.test.*",
    "!**/__tests__"
  ],
  "dependencies": {
    "@effect/cli": "0.48.25",
    "@effect/platform": "0.69.25",
    "@effect/platform-node": "0.64.27",
    "effect": "3.10.16",
    "jscodeshift": "17.1.1",
    "typescript": "^5.6.2"
  },
  "devDependencies": {
    "@types/jscodeshift": "0.12.0",
    "@types/node": "^22.9.0",
    "bunchee": "5.6.1",
    "esbuild": "0.19.2",
    "tsx": "^4.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "funding": [
    "https://trpc.io/sponsor"
  ]
}
